{
  "env": {
    "es6": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 7
  },
  "parser": "babel-eslint",
  "rules": {
    "indent": [
      "warn",
      2
    ],
    "semi": [
      "error",
      "always"
    ],
    "strict": 0,
    "no-console": "off",
    "require-yield": "off",
    "no-unused-vars": "off"
  }
}